{
  "name": "github-search",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "clean:unit-tests": "rimraf ./coverage",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "lint:fix": "npm run lint -- --fix",
    "run:unit-tests": "jest --runInBand",
    "test": "run-s clean:unit-tests \"run:unit-tests -- {1}\" --",
    "test:pattern": "run-s clean:unit-tests \"run:unit-tests -- --t {1}\" --",
    "test:update": "run-s clean:unit-tests \"run:unit-tests -- -u {1}\" --",
    "test:watch": "run-s clean:unit-tests \"run:unit-tests -- --watch {1}\" --"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --ignore-path .eslintignore --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.9.1",
    "nuxt": "^2.15.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.0.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "fibers": "^5.0.0",
    "flush-promises": "^1.0.2",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "vue-jest": "^3.0.4"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "firefox ESR",
    "ie 11"
  ]
}
