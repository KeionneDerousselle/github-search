{
  "name": "github-search",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "clean:unit-tests": "rimraf ./coverage",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:js:fix": "npm run lint -- --fix",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
    "lint": "run-p lint:js lint:style",
    "run:unit-tests": "jest --runInBand",
    "test": "run-s clean:unit-tests \"run:unit-tests -- {1}\" --",
    "test:pattern": "run-s clean:unit-tests \"run:unit-tests -- --t {1}\" --",
    "test:update": "run-s clean:unit-tests \"run:unit-tests -- -u {1}\" --",
    "test:watch": "run-s clean:unit-tests \"run:unit-tests -- --watch {1}\" --",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --ignore-path .eslintignore --fix",
    "*.{css,scss,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@octokit/rest": "^18.5.2",
    "core-js": "^3.21.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "joi": "^17.4.0",
    "nuxt": "^2.15.4",
    "uuid": "^8.3.2",
    "vee-validate": "^3.4.5",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^4.0.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.8.0",
    "fibers": "^5.0.0",
    "flush-promises": "^1.0.2",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "10.1.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.19.0",
    "vue-jest": "^3.0.7"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "firefox ESR",
    "ie 11"
  ]
}
